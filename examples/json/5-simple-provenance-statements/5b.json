{
	"prefix": {
		"default": "http://www.w3.org/ns/prov-dm/",
		"ex": "http://www.example.com/",
		"foaf": "http://xmlns.com/foaf/0.1/",
	},
	"entity": {
		"ex:post": {
			"type": "foaf:Document"
		},
		"ex:report": {
			"type": "foaf:Document"
		},
		"ex:paul": {
			"type": "foaf:Person"
		}
	},
	"agent": [ "ex:paul" ],
	"processExecution": {
		"ex:pe1": {
			"type": "ex:publicationActivity"
		}
	},
	"relation": {
		"wasSummaryOf": {
			"_:r1": {
				"cause": "ex:report",
				"effect": "ex:post"
			}
		},
		"wasAttributedTo": {
			"_:r2": {
				"XXX": "ex:post",
				"YYY": "ex:paul"
			}
		},
		"hadParticipant": {
			"_:r3": {
				"AAA": "ex:pe1",
				"BBB": "ex:paul"
			}
		}
	}
}
